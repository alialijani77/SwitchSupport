@model SwitchSupport.Domain.ViewModels.Question.FilterQuestionViewModel

<article>
    <header><h2> جدیدترین پرسش ها </h2></header>
    <ul>
        @if (Model != null && Model.Entities.Any())
        {
            @foreach (var item in Model.Entities)
            {
                <li>
                    <span> @item.ViewCount <i class="icon-star"></i></span>
                    <h2><a href="">@item.Title</a></h2>
                </li>
            }
        }
    </ul>
</article>